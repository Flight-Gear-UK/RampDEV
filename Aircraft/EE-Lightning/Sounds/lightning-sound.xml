<?xml version="1.0"?>
 
<PropertyList>

 <fx>
 
  <avon1start>
   <name>Turbine 1 Start</name>
   <mode>once</mode>
   <path>Aircraft/EE-Lightning/Sounds/engine-start.wav</path>
   <delay-sec>0.05</delay-sec>
   <condition>
    <equals>
     <property>sim/multiplay/generic/int[4]</property>
	 <value>1</value>
	</equals>
   </condition>
   <position>
    <x>-10.0</x>
    <y>0.0</y>
    <z>3.0</z>
   </position>
   <orientation>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.0</z>
    <inner-angle>180.0</inner-angle>
    <outer-angle>360.0</outer-angle>
    <outer-gain>0.2</outer-gain>
   </orientation>
   <volume>
    <property>engines/engine[0]/rpm</property>
    <factor>-0.008</factor>
    <offset>0.4</offset>
   </volume>
   <volume>
    <property>/sim/sound/eno/volume-out</property>
    <factor>1</factor>
    <offset>0</offset>
	<min>0</min>
	<max>1</max>
   </volume>
   <reference-dist>5</reference-dist>
   <max-dist>2000</max-dist>
  </avon1start>
  
  <avon1run>
   <name>Turbine 1 Run</name>
   <mode>looped</mode>
   <path>Aircraft/EE-Lightning/Sounds/engine-run.wav</path>
   <delay-sec>0.06</delay-sec>
   <condition>
    <greater-than>
     <property>engines/engine[0]/rpm</property>
	 <value>3</value>
	</greater-than>
   </condition>
   <position>
    <x>-10.0</x>
    <y>0.0</y>
    <z>3.0</z>
   </position>
   <orientation>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.0</z>
    <inner-angle>180.0</inner-angle>
    <outer-angle>360.0</outer-angle>
    <outer-gain>0.2</outer-gain>
   </orientation>
   <volume>
    <property>/sim/sound/eno/volume-out</property>
    <factor>1</factor>
   </volume>
   <volume>
    <property>engines/engine[0]/rpm</property>
    <factor>0.01</factor>
    <offset>0</offset>
	<max>0.45</max>
   </volume>
   <pitch>
    <property>engines/engine[0]/rpm</property>
	<factor>0.008</factor>
	<offset>0.3</offset>
	<min>0</min>
	<max>2.5</max>
   </pitch>
   <reference-dist>5</reference-dist>
   <max-dist>2000</max-dist>
  </avon1run>
  
  <avon1thrust>
   <name>Turbine 1 Thrust</name>
   <mode>looped</mode>
   <path>Aircraft/EE-Lightning/Sounds/thrust.wav</path>
   <delay-sec>0.05</delay-sec>
   <condition>
    <equals>
     <property>sim/multiplay/generic/int[4]</property>
	 <value>2</value>
	</equals>
   </condition>
   <position>
    <x>12.0</x>
    <y>0.0</y>
    <z>2.0</z>
   </position>
   <orientation>
	<x>1</x>
    <y>0</y>
    <z>0</z>
    <inner-angle>35.0</inner-angle>
    <outer-angle>90</outer-angle>
    <outer-gain>0.45</outer-gain>
   </orientation>
   <volume>
    <property>engines/engine[0]/rpm</property>
    <factor>0.0185</factor>
    <offset>-0.6</offset>
   </volume>
   <volume>
    <property>/sim/sound/eno/volume-out</property>
    <factor>1</factor>
    <offset>0</offset>
	<min>0</min>
	<max>1</max>
   </volume>
   
   <reference-dist>5</reference-dist>
   <max-dist>5000</max-dist>
   
  </avon1thrust>
  





<engine>
   <name>Reheat1</name>
   <mode>looped</mode>
   <path>Aircraft/EE-Lightning/Sounds/reheat2.wav</path>
   <condition>
      <greater-than>
        <property>engines/engine/reheat</property>
        <value>0.05</value>
      </greater-than>
      <not>
        <property>/sim/current-view/internal</property>
        <property>engines/engine/out-of-fuel</property>
      </not>
   </condition>
   <position>
    <x>8.0</x>
    <y>0.0</y>
    <z>2.0</z>
   </position>
   <orientation>
     <x>1</x>
    <y>0</y>
    <z>0</z>
    <inner-angle>35.0</inner-angle>
      <outer-angle>180</outer-angle>
    <outer-gain>0.60</outer-gain>
   </orientation>
   <volume>
        <property>engines/engine/reheat</property>
        <factor>2.5</factor>
        <min>0.1</min>
        <!--max>1.0</max-->
   </volume>
   <reference-dist>4</reference-dist>
   <max-dist>3000</max-dist>
</engine>
  
