
<PropertyList>

	<path>PTR-175.ac</path>
	
	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<condition>
			<property>/sim/rendering/rembrandt/enabled</property>
		</condition>
		<object-name>case.illum</object-name>
		<object-name>channel-select.illum</object-name>
		<object-name>volume.illum</object-name>
		<object-name>mode-select.illum</object-name>
		<object-name>digit.1</object-name>
  <object-name>digit.2</object-name>
  <object-name>digit.3</object-name>
  <object-name>digit.4</object-name>
  <object-name>digit.5</object-name>
	</effect>
	
	<animation>
		<type>rotate</type>
		<object-name>channel-select</object-name>
		<property>/instrumentation/PTR-175/channel-selected</property>
		<factor>-18</factor>
		<offset-deg>0</offset-deg>
		<center>
			<x-m>0.005838</x-m>
			<y-m>-0.03551</y-m>
			<z-m>-0.005052</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<object-name>channel-select.illum</object-name>
		<type>texrotate</type>
		<property>/instrumentation/PTR-175/channel-selected</property>
		<factor>18</factor>
		<offset-deg>0</offset-deg>
		<center>
			<x>0.374</x>
			<y>0.618</y>
			<z>0</z>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	
	<animation>
        <type>knob</type>
        <object-name>channel-select</object-name>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>/instrumentation/PTR-175/channel-selected</property>
                <factor>1</factor>
                <min>0</min>
                <max>20</max>                
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
	
	<animation>
		<type>rotate</type>
		<object-name>volume</object-name>
		<object-name>volume.illum</object-name>
		<property>/instrumentation/PTR-175/volume-norm</property>
		<factor>-310</factor>
		<offset-deg>0</offset-deg>
		<center>
			<x-m>0.005519</x-m>
			<y-m>0.007205</y-m>
			<z-m>-0.01191</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
        <type>knob</type>
        <object-name>volume</object-name>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>/instrumentation/PTR-175/volume-norm</property>
                <factor>0.04</factor>
                <min>0</min>
                <max>1</max>                
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
	
	<animation>
		<type>rotate</type>
		<object-name>mode-select</object-name>
		<object-name>mode-select.illum</object-name>
		<property>/instrumentation/PTR-175/mode-selected</property>
		<factor>-29</factor>
		<offset-deg>0</offset-deg>
		<center>
			<x-m>0.005519</x-m>
			<y-m>0.04419</y-m>
			<z-m>-0.009998</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
        <type>knob</type>
        <object-name>mode-select</object-name>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>/instrumentation/PTR-175/mode-selected</property>
                <factor>1</factor>
                <min>0</min>
                <max>6</max>                
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
	
	<animation>
        <type>knob</type>
        <object-name>freq-select.1</object-name>
		<property>/instrumentation/PTR-175/manual-frequency-display-10mhz</property>
		<factor>-36</factor>
		<offset-deg>0</offset-deg>
		<center>
			<x-m>0.005838</x-m>
			<y-m>-0.04404</y-m>
			<z-m>0.03165</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<action>
		<drag-scale>200</drag-scale>
            <binding>
				<command>property-adjust</command>
				<property>/instrumentation/PTR-175/manual-frequency-display-10mhz</property>
				<factor>1</factor>
				<min>10</min>
				<max>33</max>
				<wrap>false</wrap>
			</binding>
        </action>
    </animation>
	
	<animation>
        <type>knob</type>
        <object-name>freq-select.2</object-name>
		<property>/instrumentation/PTR-175/manual-frequency-display-1mhz</property>
		<factor>-36</factor>
		<offset-deg>0</offset-deg>
		<center>
			<x-m>0.005838</x-m>
			<y-m>0.001504</y-m>
			<z-m>0.01663</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<action>
            <binding>
				<command>property-adjust</command>
				<property>/instrumentation/PTR-175/manual-frequency-display-1mhz</property>
				<factor>1</factor>
				<min>0</min>
				<max>10</max>
				<wrap>true</wrap>
			</binding>
        </action>
    </animation>
	
	<animation>
        <type>knob</type>
        <object-name>freq-select.3</object-name>
		<property>/instrumentation/PTR-175/manual-frequency-display-decimal</property>
		<factor>-36</factor>
		<offset-deg>0</offset-deg>
		<center>
			<x-m>0.005838</x-m>
			<y-m>0.04385</y-m>
			<z-m>0.03165</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<action>
            <binding>
				<command>property-adjust</command>
				<property>/instrumentation/PTR-175/manual-frequency-display-decimal</property>
				<factor>1</factor>
				<min>00</min>
				<max>95</max>
				<wrap>true</wrap>
			</binding>
        </action>
    </animation>
	
	<animation>
		<type>rotate</type>
		<object-name>flap</object-name>
		<object-name>plate</object-name>
		<property>/instrumentation/PTR-175/flap-position-norm</property>
		<factor>180</factor>
		<offset-deg>0</offset-deg>
		<center>
			<x-m>0.00036</x-m>
			<y-m>0</y-m>
			<z-m>-0.05886</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>flap</object-name>
		<object-name>plate</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<interval-sec>0</interval-sec>
			<binding>
				<command>nasal</command>
				<script>uvhf.channel_flap(1)</script>
			</binding>
			<mod-up>
				<binding>
					<command>nasal</command>
					<script>uvhf.channel_flap(0)</script>
				</binding>
			</mod-up>
		</action>
 </animation>
 
 <animation>
  <type>textranslate</type>
  <object-name>digit.1</object-name>
  <property>/instrumentation/PTR-175/manual-frequency-display-10mhz</property>
  <offset>-50</offset>
  <bias>0.0000</bias>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
   <x>0</x>
   <y>-1</y>
  </axis>
 </animation>
 
 <animation>
  <type>textranslate</type>
  <object-name>digit.2</object-name>
  <property>/instrumentation/PTR-175/manual-frequency-display-10mhz</property>
  <offset>-5</offset>
  <bias>0.0000</bias>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
   <x>0</x>
   <y>-1</y>
  </axis>
 </animation>
 
 <animation>
  <type>textranslate</type>
  <object-name>digit.3</object-name>
  <property>/instrumentation/PTR-175/manual-frequency-display-1mhz</property>
  <offset>-5</offset>
  <bias>0.0000</bias>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
   <x>0</x>
   <y>-1</y>
  </axis>
 </animation>
 
 <animation>
  <type>textranslate</type>
  <object-name>digit.4</object-name>
  <property>/instrumentation/PTR-175/manual-frequency-display-decimal</property>
  <offset>-50</offset>
  <bias>0.0000</bias>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
   <x>0</x>
   <y>-1</y>
  </axis>
 </animation>
 
 <animation>
  <type>textranslate</type>
  <object-name>digit.5</object-name>
  <property>/instrumentation/PTR-175/manual-frequency-display-decimal</property>
  <offset>-5</offset>
  <bias>0.0000</bias>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
   <x>0</x>
   <y>-1</y>
  </axis>
 </animation>
	
  <animation>
  <type>material</type>
  <object-name>case.illum</object-name>
  <object-name>volume.illum</object-name>
  <object-name>mode-select.illum</object-name>
  <emission>
    <factor-prop>/instrumentation/PTR-175/illumination-norm</factor-prop>
    <red>0.7</red>
    <green>0.4</green>
    <blue>0.2</blue>
  </emission>
 </animation>
 
 <animation>
  <type>material</type>
  <object-name>digit.1</object-name>
  <object-name>digit.2</object-name>
  <object-name>digit.3</object-name>
  <object-name>digit.4</object-name>
  <object-name>digit.5</object-name>
  <emission>
    <factor-prop>/instrumentation/PTR-175/illumination-norm</factor-prop>
    <red>0.7</red>
    <green>0.4</green>
    <blue>0.2</blue>
  </emission>
 </animation>
 
 <animation>
  <type>material</type>
  <object-name>channel-select.illum</object-name>
  <emission>
    <factor-prop>/instrumentation/PTR-175/illumination-norm</factor-prop>
    <red>0.8</red>
    <green>0.5</green>
    <blue>0.4</blue>
  </emission>
 </animation>


</PropertyList>