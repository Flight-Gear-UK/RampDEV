<?xml version="1.0"?>
<!-- Regional definitions for the African rainforest, based on Tropical South America regions -->
<PropertyList>
  
  <condition>
   <and>
   <equals>
    <property>sim/startup/season</property>
    <value>summer</value>
   </equals>
    <and>
     <greater-than>
      <property>position/longitude-deg</property>
      <value>-13.23</value>
     </greater-than>
     <less-than>
      <property>position/longitude-deg</property>
      <value>30.92</value>
     </less-than>
     <greater-than>
      <property>position/latitude-deg</property>
      <value>-7.16</value>
     </greater-than>
     <less-than>
      <property>position/latitude-deg</property>
      <value>9.0</value>
     </less-than>
    </and>
   </and>
  </condition>

</PropertyList>
