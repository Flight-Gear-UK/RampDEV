<?xml version="1.0" encoding="UTF-8" ?>
<PropertyList>

 <path>egodhanger1.ac</path>
 
 <nasal>
  <load>
   if (!contains(globals, "scenery")) globals["scenery"] = {};
   if (!contains(scenery, "hanger1")) scenery["hanger1"] = {};
   if (!contains(scenery.hanger1, "hangar")) scenery.hanger1["hangar"] = {};
   if (contains(scenery, "sharedDoor")) {
     scenery.hanger1.hangar[0] =
       { door :
         [scenery.sharedDoor.new("/scenery/hanger1/hangar[0]/door[0]", 30),
          scenery.sharedDoor.new("/scenery/hanger1/hangar[0]/door[1]", 30),
		  scenery.sharedDoor.new("/scenery/hanger1/hangar[0]/door[2]", 30),
		  scenery.sharedDoor.new("/scenery/hanger1/hangar[0]/door[3]", 30)
         ]
       };
   } else {
     scenery.hanger1.hangar[0] =
       { door :
         [aircraft.door.new("/scenery/hanger1/hangar[0]/door[0]", 30),
          aircraft.door.new("/scenery/hanger1/hangar[0]/door[1]", 30),
		  aircraft.door.new("/scenery/hanger1/hangar[0]/door[2]", 30),
		  aircraft.door.new("/scenery/hanger1/hangar[0]/door[3]", 30)
         ]
       };
   }
  </load>
  <unload>
   if (contains(scenery, "sharedDoor")) {
     scenery.hanger1.hangar[0].door[0].destroy();
     scenery.hanger1.hangar[0].door[1].destroy();
	 scenery.hanger1.hangar[0].door[2].destroy();
	 scenery.hanger1.hangar[0].door[3].destroy();
   }
  </unload>
 </nasal>
 
