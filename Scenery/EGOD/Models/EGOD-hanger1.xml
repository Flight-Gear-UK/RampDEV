  <!-- LIGHTS -->
 <animation>
		<type>material</type>
		<object-name>lights1</object-name>
		<transparency>
			<alpha-prop>environment/visibility-m</alpha-prop>
			<factor>1</factor>
			<offset>0</offset>
			</transparency>
		<threshold>0.001</threshold>
	</animation>

  <animation>
    <type>select</type>
    <object-name>lights1</object-name>
    <condition>
      <equals>
        <property>controls/switches/lights1</property>
        <value> 0 </value>
      </equals>
    </condition>
  </animation> 
  
  <animation>
    <type>pick</type>
    <visible>true</visible>
	<object-name>lights1</object-name>
	<object-name>lightcase1</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/lights1</property>
      </binding>
    </action>
  </animation> 
  
   <animation>
    <type>select</type>
    <object-name>lights1</object-name>
    <condition>
      <not><property>/sim/rendering/rembrandt/enabled</property></not>
    </condition>
  </animation> 
  
  
  <!--  enable-hot  -->
 <animation>
    <object-name>hanger</object-name>
	<object-name>lights1</object-name>
	<object-name>lightcase1</object-name>
	<object-name>hangerroof1</object-name>
	<enable-hot type="bool">false</enable-hot>
  </animation> 
  
  <!-- RANGE -->
 <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
 </animation>
 
